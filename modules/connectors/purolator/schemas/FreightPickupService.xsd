        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="PickUp" nillable="true" type="tns:PickUp" />
    <xsd:complexType name="SenderInformation">
        <xsd:sequence>
            <xsd:element name="Address" nillable="true" type="tns:Address" />
