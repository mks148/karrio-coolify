    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="systemException">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="message" type="xs:string" />
                <xs:element name="timestamp" type="xs:date" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>