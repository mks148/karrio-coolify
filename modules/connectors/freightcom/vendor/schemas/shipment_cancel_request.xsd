<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.freightcom.net/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Freightcom">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="ShipmentCancelRequest">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Order">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:int" name="orderId"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute type="xs:string" name="username"/>
      <xs:attribute type="xs:string" name="password"/>
      <xs:attribute type="xs:string" name="version"/>
    </xs:complexType>
  </xs:element>
</xs:schema>